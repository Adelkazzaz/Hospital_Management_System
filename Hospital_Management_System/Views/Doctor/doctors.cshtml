@model List<demo.Models.Staff>

@{
    ViewData["Title"] = "Doctors List";
}

<h2>Doctors List</h2>

<table class="table">
    <thead>
        <tr>
            <th>Doctor ID</th>
            <th>Name</th>
            <th>Job Title</th>
            <th>Department ID</th>
            <!-- Add other relevant columns here -->
        </tr>
    </thead>
    <tbody>
        @foreach (var doctor in Model)
        {
            <tr>
                <td>@doctor.ID</td>
                <td>@doctor.FullName</td>
                <td>@doctor.JobTitle</td>
                <td>@doctor.dept_id</td>
                <!-- Add other relevant columns here -->
            </tr>
        }
    </tbody>
</table>

@foreach (var i in (List<demo.Models.Department>)ViewData["Dept"])
{
    <i>@i.Name</i>
    <i>@i.Office</i>

}
